#include <stdio.h>
#include <stdlib.h>
#include <string.h>

struct dog
{
    char type[20];
    int age;
    char name[20];
};

int main()
{
    struct dog dog1 = {"Dalmation", 5, "Spot"};
    printf("Type: %s\n", dog1.type);
    printf("Age: %d\n", dog1.age);
    printf("Name: %s\n", dog1.name);
}
