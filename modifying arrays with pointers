#include <stdio.h>
#include <stdlib.h>
#include <string.h>


void changeValue(int *a);

int main()
{
    int arr[5] = {12, 15, 12, 17, 18};
    changeValue(arr);
    for(int i = 0; i < 5; i++)
    {
        printf("%d ", arr[i]);
    }
}

void changeValue(int *a)
{
  *(a+1) = 5; //change to modify different parts of the array
}
